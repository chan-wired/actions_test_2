{
  "name": "actions_test",
  "version": "13.2.0-stage.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release:major": "npx npmversion --increment major",
    "release:minor": "npx npmversion --increment minor",
    "release:patch": "npx npmversion --increment patch",
    "release:premajor": "npx npmversion --preid stage --increment premajor",
    "release:preminor": "npx npmversion --preid stage --increment preminor",
    "release:prepatch": "npx npmversion --preid stage --increment prepatch",
    "release:prerelease": "npx npmversion --preid stage --increment prerelease"
  },
  "dependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "showdown": "^2.1.0",
    "standard-version": "^9.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableSubjectLowerCase": true
    }
  }
}
